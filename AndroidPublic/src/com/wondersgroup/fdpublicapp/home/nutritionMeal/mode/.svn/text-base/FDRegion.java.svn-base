package com.wondersgroup.fdpublicapp.home.nutritionMeal.mode;

import java.util.ArrayList;

/**
 *  行政区域
 * @author chengshaohua
 *
 */
public class FDRegion {

	private int id;
	private String name;
	private ArrayList<FDRegion> children;
	private boolean selected;
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public ArrayList<FDRegion> getChildren() {
		return children;
	}
	public void setChildren(ArrayList<FDRegion> children) {
		this.children = children;
	}
	public boolean isSelected() {
		return selected;
	}
	public void setSelected(boolean selected) {
		this.selected = selected;
	}
	
	
}
